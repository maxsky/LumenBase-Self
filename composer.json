{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-redis": "*",
        "ext-sodium": "*",
        "alibabacloud/client": "^1.5",
        "aliyuncs/oss-sdk-php": "^2.6",
        "dcat/laravel-wherehasin": "^0.8.0",
        "firebase/php-jwt": "^6.3",
        "illuminate/broadcasting": "^9.0",
        "illuminate/mail": "^9.0",
        "illuminate/redis": "^9.0",
        "kra8/laravel-snowflake": "^2.1",
        "laravel/lumen-framework": "^9.0",
        "overtrue/laravel-lang": "^6.0",
        "phpseclib/phpseclib": "^3.0",
        "propaganistas/laravel-phone": "^4.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/image-optimizer": "^1.6",
        "wechatpay/wechatpay": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.20",
        "league/flysystem": "^3.2",
        "maxsky/lumen-app-key-generator": "^1.0",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5",
        "wnx/laravel-stats": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/response.php",
            "app/Constants/SYSTEM.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
